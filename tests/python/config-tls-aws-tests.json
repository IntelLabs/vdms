    // Network
    "port": 55566,
    "cert_file": "/tmp/trusted_server_cert.pem",
    "key_file": "/tmp/trusted_server_key.pem",
    "ca_file": "/tmp/trusted_ca_cert.pem",
    "db_root_path": "test_db_tls",
    "tmp_path": "./tests_output_dir",
